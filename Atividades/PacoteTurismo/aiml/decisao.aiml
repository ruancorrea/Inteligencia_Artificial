	<!-- insert your AIML categories here -->
	
	<category>
	    <pattern>SIM</pattern>
	    <topic name="turismo"></topic>
	    <template>
	        <think>
	            <set name="sm">sim</set>
	        </think>
	        
	        <condition name="cidade" value="maceió">
                <srai>PACOTEMACEIO</srai>
	        </condition>
	        
	        <condition name="cidade" value="fortaleza">
	            <think><set name="pacote">Falésias Cearenses</set></think>
	            <srai>PACOTEFORTALEZA</srai>
	        </condition>
	        
	        <condition name="cidade" value="aracaju">
	            <think><set name="transporte">Ônibus</set></think>
	            <srai>PACOTEARACAJU</srai>
	        </condition>
	    </template>
	</category>
	
